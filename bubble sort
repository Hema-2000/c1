#include <stdio.h>

int main()
{
    int n;
  int arr[n];
  int i,j,temp;
  printf("enter value of n:");
  scanf("%d",&n);
  for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
  for(int i=0;i<n-1;i++)//this loop is for pass
      {
          for(j=0;j<n-1-i;j++)//this loop is for comparision to sort 
          {
          if(arr[j]>arr[j+1])
          {
              temp=arr[j];
              arr[j]=arr[j+1];
              arr[j+1]=temp;
          }
         }
      }
  printf("final sorted elements are:");
  for(i=0;i<n;i++)
  {
      printf("%d",arr[i]);
  }
  return 0;
}


output:
enter value of n:5
2 7 4 9 3
final sorted elements are:23479
