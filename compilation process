# 2 "comp.c" 2




# 5 "comp.c"
int main() {
    float radius, area;

    printf("Enter the radius: ");
    scanf("%f", &radius);
    area = (3.1415*radius*radius);
    printf("Area = %.2f", area);

    return 0;
}
